{% extends 'base.html.twig' %}

{% block title %}YearOfProduction index{% endblock %}

{% block body %}
    <h1>YearOfProduction index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Year</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for year_of_production in year_of_productions %}
            <tr>
                <td>{{ year_of_production.id }}</td>
                <td>{{ year_of_production.year }}</td>
                <td>
                    <a href="{{ path('year_of_production_show', {'id': year_of_production.id}) }}">show</a>
                    <a href="{{ path('year_of_production_edit', {'id': year_of_production.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('year_of_production_new') }}">Create new</a>
{% endblock %}